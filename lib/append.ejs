<% if (multi) { %>
<%- exports %>
    .forEach(function (property) {
        exports[property] = window[property];
    });
<% } else { %>
module.exports = window[<%- exports %>];
<% } %>

<% if (remove) { %>
<% if (removeMulti) { %>
<%- remove %>
    .forEach(function (property) {
        delete window[property];
    });
<% } else { %>
delete window[<%- remove %>];
<% } %>
<% } %>
